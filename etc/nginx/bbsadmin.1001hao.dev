server {
	#listen       80;
	server_name  bbsadmin.1001hao.dev;
	root /vagrant/projects/1001/bbs;

	#access_log  /z/logs/nginx/tmp.roboocap.access.log  main;
	#error_log /z/logs/nginx/tmp.roboocap.error.log; 

	location / {
		index  index.php;
	}

	location = /50x.html {
	    root   html;
	}

	location ~ \.php$ {
	    include fastcgi_params;
	    fastcgi_pass 127.0.0.1:9000;
	    fastcgi_index index.php;
	    fastcgi_param SCRIPT_FILENAME $document_root/$fastcgi_script_name;
	}
}